<nav class="navbar navbar-toggleable-md navbar-inverse fixed-top bg-inverse">
  <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <a class="navbar-brand" href="index.html">MOLi</a>
  <div class="collapse navbar-collapse" id="navbarCollapse">
    <ul class="navbar-nav mr-auto">
      <% for (var index = 0; index < nav.length; index++) { %>
        <li class="nav-item <%- current.path[0] == nav[index].link ? 'active' : '' %>">
          <a class="nav-link" href="<%= nav[index].link %><%= nav[index].link.split(":")[0] == 'http' || nav[index].link.split(":")[0] == 'https' ? '' : '.html' %>">
            <%= nav[index].title %> <span class="sr-only">(current)</span></a>
        </li>
      <% } %>
    </ul>
  </div>
</nav>
